{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.swcc.pm.SSP_PM",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"ZSSP_USER_SRV": {
				"uri": "/sap/opu/odata/sap/ZSSP_USER_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"ZSSP_COMMON_SRV": {
				"uri": "/sap/opu/odata/sap/ZSSP_COMMON_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"ZAPI_BUSINESS_PARTNER": {
				"uri": "/sap/opu/odata/sap/API_BUSINESS_PARTNER/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"CPI_API": {
				"uri": "http://swcc-prxy:8060/https://e650049-iflmap.hcisbt.sa1.hana.ondemand.com/CPI",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		},
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.swcc.pm.SSP_PM.view.app",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.swcc.pm.SSP_PM.i18n.i18n"
				}
			},
			"ZSSP_USER_SRV": {
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request",
					"useBatch": false
				},
				"dataSource": "ZSSP_USER_SRV",
				"preload": true
			},
			"ZSSP_COMMON_SRV": {
			    	"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request",
					"useBatch": false
				},
				"dataSource": "ZSSP_COMMON_SRV",
				"preload": true
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"preload": false
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.swcc.pm.SSP_PM.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed": {
					"target": [
						"notFound"
					]
				}
			},
			"routes": [{
				"name": "Routeapp",
				"pattern": "",
				"target": [
					"Targetapp"
				]
			}, {
				"name": "ModuleSelect",
				"pattern": "ModuleSelect",
				"target": [
					"ModuleSelect"
				]
			}, {
				"name": "DetailView",
				"pattern": "",
				"target": [
					"DetailView"
				]
			}, {
				"name": "ViewRequest",
				"pattern": "ViewRequest",
				"target": [
					"ViewRequest"
				]
			}, {
				"name": "LandingView",
				"pattern": "LandingView",
				"target": [
					"LandingView"
				]
			}, {
				"name": "CustomerRegistration",
				"pattern": "CustomerRegistration",
				"target": [
					"CustomerRegistration"
				]
			}, {
				"name": "HomePage",
				"pattern": "HomePage",
				"target": [
					"HomePage"
				]
			}, {
				"name": "AppHomePage",
				"pattern": "AppHomePage",
				"target": [
					"AppHomePage"
				]
			}, {
				"name": "SlaCreation",
				"pattern": "SlaCreation",
				"target": [
					"SlaCreation"
				]
			}, {
				"name": "PMRequest",
				"pattern": "PMRequest",
				"target": [
					"PMRequest"
				]
			}, {
				"name": "ViewRequest",
				"pattern": "ViewRequest",
				"target": [
					"ViewRequest"
				]
			}, {
				"name": "PMRequestView",
				"pattern": "PMRequestView",
				"target": [
					"PMRequestView"
				]
			}],
			"targets": {
				"Targetapp": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "HomePage"
				},
				"ModuleSelect": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "ModuleSelect"
				},
				"LandingView": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "ScmCreateRequest"
				},
				"DetailView": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "DetailView"
				},
				"ViewRequest": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "ViewRequest"
				},
				"CustomerRegistration": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "CustomerRegistration"
				},
				"HomePage": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "HomePage"
				},
				"SlaCreation": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "SlaCreation"
				},
				"AppHomePage": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "AppHomePage"
				},
				"PMRequest": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "PMCreateServiceRequest"
				},
				"PMRequestView": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "PMViewRequest"
				},
				"ViewRequest": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "ViewRequest"
				},

				"notFound": {
					"viewName": "NotFound"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zssp_pm/webapp",
		"_version": "1.1.0"
	}
